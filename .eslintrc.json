{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jest": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "react/jsx-filename-extension":[
        "error",
        {
          "extesions":[
            ".js",
            ".jsx"
          ]
        }
      ],
      "global-require": "off",
      "import/prefer-default-export": "off",
      "no-unused-vars":[
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
}
